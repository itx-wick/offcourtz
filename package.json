{
  "name": "offcourtz",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "release:build": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "bundleRelease:build": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "upload": "git add . && git commit -m \"new updates\" && git push origin main",
    "start": "react-native start",
    "ios": "react-native run-ios --simulator=\"iPhone 14\"",
    "setup:ios": "cd ios && pod install && cd ..",
    "clean:ios": "rm -rf ios/Pods && rm -rf ios/Podfile.lock",
    "clean": "rm -rf node_modules && rm -rf yarn.lock",
    "prettier-fix": "prettier --write app/**/*.js",
    "prettier-check": "prettier --list-different **/*.js",
    "test": "jest",
    "lint-fix": "eslint --fix .",
    "lint-check": "eslint .",
    "rename": "react-native-rename"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.5",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^1.3.4",
    "moment": "^2.29.4",
    "react": "17.0.2",
    "react-i18next": "^12.2.0",
    "react-native": "0.68.2",
    "react-native-calendars": "^1.1294.0",
    "react-native-collapsible": "^1.6.1",
    "react-native-elements": "^3.4.3",
    "react-native-flags": "^1.0.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^3.0.2",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^3.20.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-time-date-picker": "^1.2.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
